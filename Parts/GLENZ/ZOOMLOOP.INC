section .code

zoom:
;ds:si=source bitmap row start
;es:di=destination pointer
;ax=zoom factor (desired screen width)
cmp ax,340
jae zoom_ret
xor cx,cx
test ax,4
jnz @@1
add di,2
mov cx,1
@@1:
mov bx,ax
and bx,~ 1
xor edx,edx
jmp [zoomloopt+ebx]
zoom_ret:
ret

